{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "start": {
    "require": [
      [
        "@babel/register",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": "current"
                },
                "modules": false
              }
            ],
            "@babel/preset-typescript"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import"
          ],
          "extensions": [
            ".ts",
            ".js"
          ]
        }
      ]
    ],
    "reporter": "@start/reporter-verbose"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@start/cli": "^0.3.1",
    "@start/plugin-env": "^0.3.1",
    "@start/plugin-find": "^0.2.1",
    "@start/plugin-lib-babel": "^0.2.2",
    "@start/plugin-lib-eslint": "^0.3.1",
    "@start/plugin-lib-typescript-check": "^0.2.2",
    "@start/plugin-lib-typescript-generate": "^0.3.0",
    "@start/plugin-overwrite": "^0.2.1",
    "@start/plugin-parallel": "^0.2.1",
    "@start/plugin-read": "^0.2.1",
    "@start/plugin-remove": "^0.2.2",
    "@start/plugin-rename": "^0.2.1",
    "@start/plugin-sequence": "^0.2.2",
    "@start/plugin-write": "^0.2.1",
    "@start/reporter-verbose": "^0.1.0",
    "@types/node": "^10.9.4",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^18.0.0"
  }
}
